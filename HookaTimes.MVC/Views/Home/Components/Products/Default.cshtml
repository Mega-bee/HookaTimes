@model List<Product_VM>
<div class="block block-products block-products--layout--large-last" id="products" data-mobile-grid-columns="2">
    <div class="container">
        <div class="white-background">
            <div class="block-header">
                <img src="https://uploads-ssl.webflow.com/62d93fe56471ce4a6284ae8f/62d94d34860393c6dfcf9a9f_Path%2032.svg" loading="lazy" alt="" class="icons">
                <h2 class="_1-6-h2">HookaTimes Products</h2> @* <h3 class="block-header__title">Hooka Products</h3>
                <div class="block-header__divider"></div>*@
            </div>
            <div class="block-products__body">
                <div class="block-products__list">
                    @foreach (var item in Model)
                    {
                        <partial name="../../../shared/Ecommerce/_HookaProductPartial.cshtml" model="item" />
                    }
                </div>

            </div>
            <a asp-controller="Home" asp-action="HookaProducts" class="see-all_button w-button">See All</a>
        </div>
    </div>
</div>

